{
  "name": "@mono/api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "type": "module",
  "scripts": {
    "build": "tscc --project ./tsconfig.build.json",
    "start": "tscc --project ./tsconfig.build.json && node dist/main",
    "start:dev": "tscc -w --project ./tsconfig.build.json",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,libs,test}/**/*.ts\" --fix",
    "test:e2e": "tscc --project ./tsconfig.test.json && node --experimental-vm-modules node_modules/jest/bin/jest.js --config ./dist_test/tests/e2e/core/impl/jest/runner/jest-e2e.js",
    "test:unit": "tscc --project ./tsconfig.test.json && node --test ./dist_test/tests/unit/core/impl/node-testing/runner/setup.js",
    "test:unit:mocha": "tscc --project ./test-configs/tsconfig.test.mocha.json && mocha --recursive ./dist_test/tests/unit/groups/**/**/group.tests.js ./dist_test/tests/unit/features/**/feature.js",
    "test:unit:jasmine": "tscc --project ./test-configs/tsconfig.test.jasmine.json && pnpx jasmine --config=./jasmine.json",
    "test:unit:vitest": "tscc --project ./test-configs/tsconfig.test.vitest.json && vitest run --config ./dist_test/tests/unit/core/impl/vitest/runner/vitest.js",
    "test:unit:japa": "tscc --project ./test-configs/tsconfig.test.japa.json && node ./dist_test/tests/unit/core/impl/japa/runner/setup.js",
    "dependencies-graph": "npx depcruise src --include-only \"^src\" --config --output-type dot | dot -T svg > dependency-graph.svg"
  },
  "imports": {
    "#corejest": "./dist_test/tests/unit/core/impl/jest/index.js",
    "#coremocha": "./dist_test/tests/unit/core/impl/mocha/index.js",
    "#corejapa": "./dist_test/tests/unit/core/impl/japa/index.js",
    "#corevitest": "./dist_test/tests/unit/core/impl/vitest/index.js",
    "#corejasmine": "./dist_test/tests/unit/core/impl/jasmine/index.js",
    "#corenode": "./dist_test/tests/unit/core/impl/node-testing/index.js"
  },
  "dependencies": {
    "@mono/array-methods": "workspace:*",
    "@mono/date-methods": "workspace:*",
    "@mono/number-methods": "workspace:*",
    "@mono/object-utils": "workspace:^0.0.1",
    "@mono/pattern-matching": "workspace:*",
    "@mono/promise-methods": "workspace:*",
    "@mono/string-methods": "workspace:*",
    "@mono/types-utils": "workspace:*",
    "@nestjs/common": "^9.4.0",
    "@nestjs/core": "^9.4.0",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schedule": "^4.1.0",
    "@nestjs/swagger": "^6.1.4",
    "@nestjs/throttler": "^4.0.0",
    "async-limiter": "^2.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "glob": "^10.3.10",
    "js-sha256": "^0.9.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@japa/expect": "^3.0.2",
    "@japa/runner": "^3.1.4",
    "@jest/globals": "^29.7.0",
    "@mono/compiler": "workspace:*",
    "@mono/eslint-config": "workspace:^1.0.0",
    "@mono/test-utils": "workspace:*",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.2.1",
    "@types/express": "^4.17.13",
    "@types/glob": "^8.0.1",
    "@types/jasmine": "^5.1.4",
    "@types/mocha": "^10.0.7",
    "@types/node": "^20.14.11",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^9.0.0",
    "jasmine": "^5.1.0",
    "mocha": "^10.6.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-loader": "^9.2.3",
    "tsconfig-paths": "4.1.1",
    "vitest": "^0.34.6"
  }
}
